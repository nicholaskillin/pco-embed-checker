<!DOCTYPE html>
<html lang="en">
    <%= render 'layouts/head' %>

  <body>
    <div class="container">
      <%= render partial: "shared/header" %>

      <% flash.each do | message_type, message| %>
        <div class="alert alert-<%= message_type %>"><%= message %></div>
      <% end %>
      
      <%= yield %>
    </div>
  </body>
</html>
